{
	"$schema": "https://turborepo.org/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"check": {
			"outputs": []
		},
		"check-types": {
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"format": {
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"start": {
			"cache": false,
			"passThroughEnv": [
				"BACKEND_PORT",
				"STYTCH_PROJECT_ID",
				"STYTCH_SECRET",
				"DB_URL",
				"TOKEN_KEY",
				"GEMINI_API_KEY",
				"API_URL"
			]
		}
	}
}
